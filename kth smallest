import java.util.*;
class Solution {
    public static void main(String[] args) {
        int n,i;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        int pos=s.nextInt();
        int a[]=new int[n];
        for(i=0;i<n;i++){
            a[i]=s.nextInt();
        }
        Arrays.sort(a);
        System.out.println(a[pos-1]);
                
            }
        }
