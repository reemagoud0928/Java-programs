import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int n;
   n=s.nextInt();
    for(int i=n;i>=1;i--){
        
        for(int j=i;j<=n-1;j++){
         System.out.printf(" ");
        }
    for(int k=1;k<=i;k++){
       System.out.printf("*");
}
   System.out.printf("\n");
    }
}
}
